{
    "name": "sven/helpers",
    "description": "A collection of useful helper functions for in Laravel applications.",
    "keywords": ["helpers", "functions", "useful", "development"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "graham-campbell/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Sven\\Helpers\\": "src/"
        },
        "files": ["bootstrap/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\Helpers\\Tests\\": "tests/"
        }
    }
}
